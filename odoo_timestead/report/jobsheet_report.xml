<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- QWeb Reports -->
        <record id="client_jobsheets" model="ir.actions.report">
            <field name="name">Jobsheets</field>
            <field name="model">client.jobsheet</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">odoo_timestead.report_jobsheet</field>
            <field name="report_file">odoo_timestead.report_jobsheet</field>
            <field name="print_report_name">'%s' %(object.name)</field>
            <field name="binding_model_id" ref="model_client_jobsheet"/>
            <field name="binding_type">report</field>
        </record>

        <record id="action_print_task" model="ir.actions.report">
            <field name="name">Task</field>
            <field name="model">project.task</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">odoo_timestead.report_task_ts</field>
            <field name="report_file">odoo_timestead.report_task_ts</field>
            <field name="print_report_name">'%s' %(object.name)</field>
            <field name="binding_model_id" ref="model_project_task"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>
