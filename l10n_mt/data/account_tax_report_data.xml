    <record id="account_tax_report_line_tpurv" model="account.tax.report.line">
        <field name="name">Total Purchases VAT</field>
        <field name="code">TPURV</field>
        <field name="formula">P18V+P05V+P00V+PEUV+OEUV+FEUV+F18V+F07V+F05V+F00V+O18V+O07V+O05V+O00V</field>
        <field name="sequence" eval="52"/>
        <field name="report_id" ref="tax_report"/>
    </record>

    <record id="account_tax_report_line_tliav" model="account.tax.report.line">
        <field name="name">Net VAT Liability</field>
        <field name="code">TLIAV</field>
        <field name="formula">
            (S18V+S07V+S05V+S00V+S99V+SEUV)-(P18V+P05V+P00V+PEUV+OEUV+FEUV+F18V+F07V+F05V+F00V+O18V+O07V+O05V+O00V)
        </field>
        <field name="sequence" eval="53"/>
        <field name="report_id" ref="tax_report"/>
    </record>

