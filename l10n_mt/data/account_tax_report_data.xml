    <record id="account_tax_report_line_icsubn" model="account.tax.report.line">
        <field name="name">Intra Community: Value</field>
        <field name="code">ICSUBN</field>
        <field name="formula">SEUN + PEUN + OEUN + FEUN</field>
        <field name="sequence" eval="41"/>
        <field name="report_id" ref="tax_report"/>
    </record>

    <record id="account_tax_report_line_icsubv" model="account.tax.report.line">
        <field name="name">Intra Community: VAT</field>
        <field name="code">ICSUBV</field>
        <field name="formula">SEUV+PEUV+OEUV+FEUV</field>
        <field name="sequence" eval="42"/>
        <field name="report_id" ref="tax_report"/>
    </record>

    <record id="account_tax_report_line_locssubn" model="account.tax.report.line">
        <field name="name">Local Sales: Value</field>
        <field name="code">LOCSSUBN</field>
        <field name="formula">S18N+S07N+S05N+S00N+S99N</field>
